{
  "name": "gulp-format-md",
  "description": "Gulp plugin for beautifying markdown using pretty-remarkable.",
  "version": "2.0.0",
  "homepage": "https://github.com/jonschlinkert/gulp-format-md",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/gulp-format-md",
  "bugs": {
    "url": "https://github.com/jonschlinkert/gulp-format-md/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "pretty-remarkable": "^1.0.0",
    "remarkable": "^1.7.1",
    "sections": "^1.0.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "mocha": "^5.2.0"
  },
  "keywords": [
    "beautify",
    "format",
    "formatter",
    "gulp",
    "gulpplugin",
    "markdown",
    "md",
    "plugin",
    "prettify",
    "pretty",
    "remarkable"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "markdown-toc",
        "pretty-remarkable",
        "remarkable"
      ]
    },
    "plugins": ["./"],
    "lint": {
      "reflinks": true
    }
  }
}
