{
  "name": "@aetherinox/noxkit",
  "description": "Library with helper functions for Aetherinox",
  "version": "1.1.1",
  "homepage": "https://github.com/Aetherinox/noxkit",
  "author": "Aetherinox (https://github.com/Aetherinox)",
  "contributors": [
    "Aetherinox (https://github.com/Aetherinox)"
  ],
  "publishConfig": {
    "aetherinox:registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aetherinox/noxkit.git"
  },
  "bugs": {
    "url": "https://github.com/Aetherinox/noxkit/issues"
  },
  "license": "MIT",
  "files": [
    "noxkit.d.ts",
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=16.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "js-yaml": "^3.13.1",
    "kind-of": "^6.0.2",
    "section-matter": "^1.0.0",
    "strip-bom-string": "^1.0.0"
  },
  "devDependencies": {
    "ansi-green": "^0.1.1",
    "benchmarked": "^2.0.0",
    "coffeescript": "^2.2.3",
    "delimiter-regex": "^2.0.0",
    "extend-shallow": "^3.0.2",
    "front-matter": "^2.3.0",
    "gulp-format-md": "^2.0.0",
    "minimist": "^1.2.0",
    "mocha": "^10.3.0",
    "toml": "^2.3.3",
    "vinyl": "^2.1.0",
    "write": "^1.0.3"
  },
  "keywords": [
    "yfm"
  ],
  "browser": {
    "fs": false
  },
  "typings": "noxkit.d.ts",
  "eslintConfig": {
    "rules": {
      "no-console": 0
    }
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "helpers": {
      "examples": "./examples/helper.js"
    },
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": [
        "assemble",
        "metalsmith",
        "verb"
      ]
    },
    "reflinks": [
      "coffe-script",
      "generate",
      "js-yaml",
      "toml",
      "update"
    ]
  },
  "directories": {
    "lib": "lib"
  }
}
